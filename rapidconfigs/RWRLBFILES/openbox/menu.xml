<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="apps-browsers-menu" label="Browsers">
	<item label="Firefox">
		<action name="Execute">
			<command>firefox</command>
		</action>
	</item>
	<item label="Chrome">
		<action name="Execute">
			<command>google-chrome</command>
		</action>
	</item>
	<item label="Chrome (No Sandbox)">
		<action name="Execute">
			<command>google-chrome --no-sandbox</command>
		</action>
	</item>
</menu>

<menu id="apps-fileman-menu" label="File Managers">
	<item label="XFE">
		<action name="Execute">
			<command>xfe</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<item label="Midnight Commander (Eterm)">
		<action name="Execute">
			<command>Eterm -T MidnightCommander -g 160x48 -e mc</command>
		</action>
	</item>
</menu>

<menu id="apps-editors-menu" label="Editors">
	<item label="Bluefish">
		<action name="Execute">
			<command>bluefish</command>
		</action>
	</item>
	<item label="Geany">
		<action name="Execute">
			<command>geany</command>
		</action>
	</item>
	<item label="Netbeans">
		<action name="Execute">
			<command>netbeans</command>
		</action>
	</item>
	<item label="VSCode">
		<action name="Execute">
			<command>/usr/bin/vscode</command>
		</action>
	</item>
	<item label="Medit">
		<action name="Execute">
			<command>medit</command>
		</action>
	</item>
	<item label="XFileWrite">
		<action name="Execute">
			<command>xfw</command>
		</action>
	</item>
	<item label="KeepNote">
		<action name="Execute">
			<command>keepnote</command>
		</action>
	</item>
	<item label="LibreOffice">
		<action name="Execute">
			<command>LO</command>
		</action>
	</item>
</menu>

<menu id="apps-office-menu" label="Office">
	<item label="LibreOffice">
		<action name="Execute">
			<command>libreoffice</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<item label="LibreOffice Base">
		<action name="Execute">
			<command>libreoffice --base</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<item label="LibreOffice Calc">
		<action name="Execute">
			<command>libreoffice --calc</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<item label="LibreOffice Draw">
		<action name="Execute">
			<command>libreoffice --draw</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<item label="LibreOffice Impress">
		<action name="Execute">
			<command>libreoffice --impress</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<item label="LibreOffice Math">
		<action name="Execute">
			<command>libreoffice --math</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<item label="LibreOffice Writer">
		<action name="Execute">
			<command>libreoffice --writer</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
</menu>

<menu id="apps-terminals-menu" label="Terminals">
	<item label="xterm">
		<action name="Execute">
			<command>xterm</command>
		</action>
	</item>
	<item label="Hyper">
		<action name="Execute">
			<command>hyper</command>
		</action>
	</item>
	<item label="ST">
		<action name="Execute">
			<command>st -f DejaVuSansMono</command>
		</action>
	</item>
	<item label="Eterm">
		<action name="Execute">
			<command>Eterm --double-buffer</command>
		</action>
	</item>
	<item label="Eterm Black">
		<action name="Execute">
			<command>Eterm --double-buffer -P none</command>
		</action>
	</item>
</menu>

<menu id="apps-utils-menu" label="Utilities">
	<item label="Gkrellm">
		<action name="Execute">
			<command>gkrellm</command>
		</action>
	</item>
	<item label="Fox Calculator">
		<action name="Execute">
			<command>calculator</command>
		</action>
	</item>
	<item label="PulseAudio Volume Control">
		<action name="Execute">
			<command>pavucontrol</command>
		</action>
	</item>
	<item label="calc (Eterm)">
		<action name="Execute">
			<command>Eterm -T calc -g 100x30 -e calc</command>
		</action>
	</item>
	<item label="Htop (Eterm)">
		<action name="Execute">
			<command>Eterm -T Htop -g 160x48 -e htop</command>
		</action>
	</item>
	<item label="AlsaMixer (Eterm)">
		<action name="Execute">
			<command>Eterm -T AlsaMixer -g 160x48 -e alsamixer</command>
		</action>
	</item>
	<item label="Graveman">
		<action name="Execute">
			<command>graveman</command>
		</action>
	</item>
</menu>

<menu id="apps-graphics-menu" label="Graphics">
	<item label="Gimp">
		<action name="Execute">
			<command>gimp</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<item label="Inkscape">
		<action name="Execute">
			<command>inkscape</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
</menu>

<menu id="apps-RE-menu" label="Reverse Engineering">
	<item label="Cutter">
		<action name="Execute">
			<command>cutter</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
</menu>

<menu id="system-menu" label="System">
	<item label="Openbox Configuration Manager">
		<action name="Execute">
			<command>obconf</command>
			<startupnotify><enabled>yes</enabled></startupnotify>
		</action>
	</item>
	<separator />
	<item label="Reconfigure Openbox">
		<action name="Reconfigure" />
	</item>
</menu>

<menu id="root-menu" label="Openbox 3">
	<separator label="Quick Access" />
	<item label="xterm"><action name="Execute"><command>xterm</command></action></item>
	<item label="Hyper"><action name="Execute"><command>hyper</command></action></item>
	<item label="Eterm Black"><action name="Execute"><command>Eterm --double-buffer -P none</command></action></item>
	<separator label="Applications" />
	<menu id="apps-browsers-menu"/>
	<menu id="apps-fileman-menu"/>
	<menu id="apps-editors-menu"/>
	<menu id="apps-office-menu"/>
	<menu id="apps-terminals-menu"/>
	<menu id="apps-utils-menu"/>
	<menu id="apps-graphics-menu"/>
	<menu id="apps-RE-menu"/>
	<separator label="System" />
	<menu id="system-menu"/>
	<separator />
	<item label="Log Out">
		<action name="Exit">
			<prompt>yes</prompt>
		</action>
	</item>
</menu>

</openbox_menu>
